<xsd:schema targetNamespace="http://www.garmin.com/xmlschemas/GpxExtensions/v3"
  elementFormDefault="qualified"
  xmlns="http://www.garmin.com/xmlschemas/GpxExtensions/v3"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <xsd:complexType name="Pair">
    <xsd:simpleContent>
      <xsd:extension base="xsd:token">
        <xsd:attribute name="Name" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>A name/value pair of Stings</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
</xsd:schema>
